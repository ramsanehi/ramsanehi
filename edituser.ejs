                    EmailAddress:<input type="email" name="emailAddress" value="<%=data.emailAddress%>"  id="email" placeholder="EmailAddress"><br><br>
                    
                  <label for="address" ></label>
                  Address:<input type="text" name="Address" value="<%=data.Address%>" id="add" placeholder="Address"><br><br>
              
                  <input type="submit" name="submit" class="btn-primary" class="form-control">

</div>
                    </div>
</div>
                   
                    </form>
                    

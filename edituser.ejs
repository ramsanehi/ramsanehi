        <form role="form" method="post" action="/api/edituser/<%=data.id%>" id="contact-form" class="contact-form">
         <div class="row">
                <div class="col-md-6">
                    <div class="form-group">
            <label for="Username"></label> 
                   UserName:<input type="text" name="userName" value="<%=data.userName%>" id="uname" placeholder="Username"><br><br>
                   
                   <label for="password"></label>
                      
                    Password:<input type="password" name="Password" value="<%=data.password%>" id="pass" placeholder="password"><br><br>
                    </div>
            
                    <label for="mobileno"></label>
                 MobileNo:<input type="text" name="mobileNo" value="<%=data.mobileNo%>" id="no" placeholder="MobileNO"><br><br>
                 
